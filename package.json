        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "bootstrap": "^4.5.0",
        "cropperjs": "^1.5.7",
        "express": "^4.17.1",
        "jquery": "^3.5.1",
        "jquery-datetimepicker": "^2.5.21",
        "jquery-slimscroll": "^1.3.8",
        "jquery-ui": "^1.12.1",
        "popper.js": "^1.16.1",
        "socket.io": "^2.4.0",
        "socket.io-client": "^2.3.0"
    }
